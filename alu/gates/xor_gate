    %set/v v0x8f4950_0, 0, 1;
    %set/v v0x8f4a20_0, 0, 1;
    %delay 1, 0;
    %set/v v0x8f4950_0, 0, 1;
    %set/v v0x8f4a20_0, 1, 1;
    %delay 1, 0;
    %set/v v0x8f4950_0, 1, 1;
    %set/v v0x8f4a20_0, 0, 1;
    %delay 1, 0;
    %set/v v0x8f4950_0, 1, 1;
    %set/v v0x8f4a20_0, 1, 1;
    %end;
    .thread T_0;
