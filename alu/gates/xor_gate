v0x8f4a20_0 .var "B", 0 0;
v0x8f4ad0_0 .net "Z", 0 0, L_0x8f4b80; 1 drivers
S_0x8d0ab0 .scope module, "xor_gate" "xor_gate" 2 6, 3 6, S_0x8d09c0;
 .timescale 0 0;
