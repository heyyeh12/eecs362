v0x8f48b0_0 .alias "z", 0 0, v0x8f4ad0_0;
    .scope S_0x8d09c0;
T_0 ;
    %vpi_call 2 9 "$monitor", "A = %b B = %b Z = %b", v0x8f4950_0, v0x8f4a20_0, v0x8f4ad0_0;
